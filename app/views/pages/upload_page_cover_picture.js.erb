
<% if @page.errors.any? %>
  alert(<%= @page.errors.full_messages %>)
<% else %>
 <% if @page.check_if_profile_is_complete? %>
  $('.timeline-panel').hide('slow')
  $('.time-line').css('height','315');
  $('.add-image').css('top','140px');
 <% end %> 
 $('.time-line').css({'background-image':'url(<%= @page.cover_picture.url %>)'});
<% end %>