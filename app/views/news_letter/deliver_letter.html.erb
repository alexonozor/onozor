<html>
<head>
  <meta http-equiv="Content-type" content="text/html; charset=utf-8">
  <%= stylesheet_link_tag 'application',  'https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css', media: "all"%>
  <title>Onozor Digest</title>
</head>
<body>
<div class="container">
  <div class="row">
   <% @latest_question.each do |question| %>
    <div class="col-md-12">
      <h3><%= question_url (question) %></h3>
      <p class="text-left">
        <%= question.body %>
      </p>
    </div>
    <% end %>
  </div>
</div>
</body>
</html>