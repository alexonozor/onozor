<% current_user.activities.each do |activity| %>
<li class='<%= activity.seen %>' id="note" >
  <% if activity.trackable.present? %>
          <%= div_for(activity, class: "foo") do %>
              <%= render "activities/#{activity.trackable_type}/#{activity.action}", activity: activity %>
          <% end %>
     <% end %>
    <li>
<% end %>
