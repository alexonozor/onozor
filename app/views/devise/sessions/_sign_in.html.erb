<%= form_for(resource,  url: session_path(resource_name)) do |f| %>

<div class="col-lg-12 tos_disclaimer">
  <p><b> LOG IN</b></p>
</div>
<div class="col-lg-12">
  <div class="form-inputs">
     <div class="form-group">
       <%= f.email_field :email, required: true, :label=> :false, :class=>"form-control", :placeholder=>"Email" %>
     </div>
  </div>


   <div class="form-inputs">
        <div class="form-group">
     <%= f.password_field :password, required: true, :label=> :false, :class=>"form-control", :placeholder=>"Password" %>
   </div>
  </div>


      <div class="form-actions">
        <button class="btn btn-success">Sign in</button>
      </div>
</div>

<% end %>
