    NO MAXVALUE
    CACHE 1;


ALTER TABLE users_id_seq OWNER TO obogbarealexonozor;

--
-- Name: users_id_seq; Type: SEQUENCE OWNED BY; Schema: public; Owner: obogbarealexonozor
--

ALTER SEQUENCE users_id_seq OWNED BY users.id;


--
-- Name: versions; Type: TABLE; Schema: public; Owner: obogbarealexonozor
--

CREATE TABLE versions (
    id integer NOT NULL,
    item_type character varying(255) NOT NULL,
    item_id integer NOT NULL,
    event character varying(255) NOT NULL,
    whodunnit character varying(255),
    object text,
    created_at timestamp without time zone
);


ALTER TABLE versions OWNER TO obogbarealexonozor;

--
-- Name: versions_id_seq; Type: SEQUENCE; Schema: public; Owner: obogbarealexonozor
--

CREATE SEQUENCE versions_id_seq
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


ALTER TABLE versions_id_seq OWNER TO obogbarealexonozor;

